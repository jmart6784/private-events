<h1>All Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @user.each do |users| %>
        <tr>
          <td><%= users.email %></td>
          <td><%= link_to 'Show', users %></td><br>
        </tr>
      <% end %>
  </tbody>
</table>

